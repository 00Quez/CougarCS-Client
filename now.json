{
    "version": 2,
    "builds": [
        {
            "src": "package.json",
            "use": "@now/static-build",
            "config": {
                "distDir": "build"
            }
        }
    ],
    "alias": [
        "uhcougarcstest.now.sh"
    ],
    "routes": [
        {
            "src": "^/static/(.*)",
            "dest": "/static/$1"
        },
        {
            "src": ".*",
            "dest": "/index.html"
        }
    ]
}